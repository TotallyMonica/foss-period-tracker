package tech.monicarose.foss_period_tracker.date;

public class Stored
{
	final private String _DATE_FORMAT = "YYYY/MM/DD";
	private int _timeZone = 0;
	private String _date = "";
	
	public Stored()
	{
		this(0, "");
	}
	
	public Stored(int timeZone)
	{
		this(timeZone, "");
	}
	
	public Stored(String date)
	{
		this(0, date);
	}
	
	public Stored(int timeZone, String date)
	{
		_timeZone = timeZone;
		_date = date;
	}
	
	public static String toString()
	{
		String output = "";
		return output;
	}
}
