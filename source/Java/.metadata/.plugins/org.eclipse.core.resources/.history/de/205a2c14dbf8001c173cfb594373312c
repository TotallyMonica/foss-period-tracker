package tech.monicarose.foss_period_tracker.date;
import tech.monicarose.foss_period_tracker.date.Date;

public class Stored extends Date
{
	final private String _DATE_FORMAT = "YYYY/MM/DD";
	private int _timeZone = 0;
	Date saved;
	
	public Stored()
	{
		this(0, "");
	}
	
	public Stored(int timeZone)
	{
		this(timeZone, "");
	}
	
	public Stored(String date)
	{
		this(0, date);
	}
	
	public Stored(int timeZone, String date)
	{
		saved = new Date();
		_timeZone = timeZone;
		_date = date;
	}
	
	public String toStoredFormat(String date)
	{
		return date;
	}
	
	public String toString()
	{
		return Date()
	}
}
